{
  "name": "altrabot",
  "version": "1.0.0",
  "description": "WhatsApp Bot dengan 200 Perintah",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prod": "ts-node src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:reset": "prisma migrate reset",
    "clean": "rm -rf dist node_modules package-lock.json",
    "install:clean": "npm run clean && npm install",
    "docker:build": "docker build -t altrabot .",
    "docker:run": "docker run -p 3000:3000 altrabot",
    "docker:clean": "docker system prune -f",
    "test": "echo 'No tests yet' && exit 0",
    "lint": "echo 'No linter configured' && exit 0",
    "typecheck": "tsc --noEmit",
    "healthcheck": "curl -f http://localhost:3000/healthz || exit 1"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@whiskeysockets/baileys": "^6.4.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.10",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "qrcode": "^1.5.3",
    "ts-node": "^10.9.1",
    "fluent-ffmpeg": "^2.1.2",
    "sharp": "^0.32.6",
    "pdf-lib": "^1.17.1",
    "tesseract.js": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.8",
    "@types/uuid": "^9.0.5",
    "@types/jest": "^29.5.6",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0",
    "prisma": "^5.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "whatsapp-bot",
    "baileys",
    "nodejs",
    "typescript"
  ],
  "author": "AltraBot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/altrabot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/altrabot/issues"
  },
  "homepage": "https://github.com/your-username/altrabot#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
